<?xml version="1.0" encoding="utf-8"?>

<resources>

    <declare-styleable name="DaxButton">
        <attr name="buttonSize" format="enum">
            <enum name="small" value="0" />
            <enum name="large" value="1" />
        </attr>
    </declare-styleable>

    <attr name="daxButtonPrimary" format="reference" />
    <attr name="daxButtonSecondary" format="reference" />
    <attr name="daxButtonGhost" format="reference" />
    <attr name="daxButtonDestructive" format="reference" />
    <attr name="daxButtonGhostDestructive" format="reference" />

    <attr name="radioButtonStyle" format="reference" />

</resources>