<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="NotifyMeView">
        <attr name="primaryText" />
        <attr name="secondaryText" />
        <attr name="sharedPrefsKeyForDismiss" format="string" />
        <attr name="dismissIcon" format="boolean" />
        <attr name="contentOrientation" format="enum">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
        </attr>
    </declare-styleable>
</resources>